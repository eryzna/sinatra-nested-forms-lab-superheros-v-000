<h1>Create a Team and Heroes!</h1>

<form action="/teams" method="post">
  <input type="text" name="team[name]" value="team_name">
  <input type="text" name="team[motto]" value="team_motto">

  <h2>Hero 1</h2>
  <input type="text" name="team[name][][name]" value="member1_name" id="member1_name">
  <input type="text" name="team[name][][power]" value="member1_power">
  <input type="text" name="team[name][][bio]" value="member1_bio">

  <h2>Hero 2</h2>
  <input type="text" name="team[name][][name]" value="superhero_2_name">
  <input type="text" name="team[name][][power]" value="superhero_2_power">
  <input type="text" name="team[name][][bio]" value="superhero_2_bio">

  <h2>Hero 3</h2>
  <input type="text" name="team[name][][name]" value="superhero_3_name">
  <input type="text" name="team[name][][power]" value="superhero_3_power">
  <input type="text" name="team[name][][bio]" value="superhero_3_bio">
  <input type="submit" name="submit" value="Submit">


</form>
